def test(actual, expected):

    # you can reveal a number in order to print it

    actual = actual.reveal()
    print_ln('expected %s, got %s', expected, actual)

sfix.set_precision(31, 63)
a = sfix(10000)
b = sfix(3333)

print_ln('using original div')
c = a/b
test(c , 3.333)

# target function.
@GFA(10, 31, 63, (1,100000))
def inv(x):
    return 1 / x

print_ln('using GFA div')
c = a * inv(b)
test(c , 3.333)


